<% title "Ticket" %>

<p>
  <strong>Datum:</strong>
  <%=h @ticket.datum %>
</p>
<p>
  <strong>Bearbeiter:</strong>
  <%=h @ticket.owner %>
</p>
<p>
  <strong>Ticketnummer:</strong>
  <%=h @ticket.ticketnummer %>
</p>
<p>
  <strong>Prio:</strong>
  <%=h @ticket.prio %>
</p>
<p>
  <strong>Serial:</strong>
  <%=h @ticket.serial %>
</p>
<p>
  <strong>Sunid:</strong>
  <%=h @ticket.sunID %>
</p>
<p>
  <strong>Langtext:</strong>
  <%=h @ticket.langtext %>
</p>

<p>
  <%= link_to "Edit", edit_ticket_path(@ticket) %> |
